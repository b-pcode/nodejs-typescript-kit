{
  "extends": ["airbnb", "prettier"],
  "env": { "jest": true },
  "globals": {
    "afterAll": true,
    "afterEach": true,
    "beforeAll": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "it": true,
    "jest": true
  },
  "settings": {
    "plugins": ["import"],
    "rules": {
      "import/no-unresolved": "error"
    },
    "import/resolver": {
      "typescript": {}
    }
  }
}
